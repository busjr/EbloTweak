{
    "manifest_version": 3,
    "name": "EbloTweak (Дополнительные настройки для Eblo.id)",
    "version": "2.2",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16x16.png",
        "32": "icons/icon32x32.png",
        "48": "icons/icon48x48.png",
        "128": "icons/icon128x128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://eblo.id/*"
            ],
            "css": [
                "theme.css"
            ],
            "js": [
                "config.js",
                "modules.js",
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}